<div class="mws-panel grid_3 mws-collapsible">
    <div class="mws-panel-header">
        <span><i class="icon-rss"></i> Server Status</span>
    </div>
    <div class="mws-panel-body no-padding">
        <ul class="mws-summary clearfix">
            <li>
                <span class="key"><i class="icon-globe"></i> Currently Playing</span>
                <span class="val">
                    <span class="text-nowrap">{server.mapname} ({server.bf2_mapsize})</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-flag"></i> Game Mode</span>
                <span class="val">
					<span class="text-nowrap">{server.gametype}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-power-2"></i> Status</span>
                <span class="val">
					<span class="text-nowrap">{server.gamemode}</i></span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-folder-close"></i> Game Mod</span>
                <span class="val">
					<span class="text-nowrap">{server.gamevariant}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-users"></i> Players</span>
                <span class="val">
					<span class="text-nowrap">{server.numplayers} / {server.maxplayers}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-watch2"></i> Round Time</span>
                <span class="val">
					<span class="text-nowrap">{server.roundtime}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-flag"></i> Team 1 Score</span>
                <span class="val">
					<span class="text-nowrap">{server.team1score}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-flag2"></i> Team 2 Score</span>
                <span class="val">
					<span class="text-nowrap">{server.team2score}</span>
				</span>
            </li>
        </ul>
    </div>
</div>

<div class="mws-panel grid_3 mws-collapsible clear">
    <div class="mws-panel-header">
        <span><i class="icon-book"></i> Server Information</span>
    </div>
    <div class="mws-panel-body no-padding">
        <ul class="mws-summary clearfix">
            <li>
                <span class="key"><i class="icon-broadcast-2"></i> Server Version</span>
                <span class="val">
					<span class="text-nowrap">{server.gamever}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-server"></i> Dedicated</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_dedicated}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-windows"></i> Operating System</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_os}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-rating3"></i> Ranked</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_ranked}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-gun"></i> Unlocks</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_globalunlocks}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-key-2"></i> Has Password</span>
                <span class="val">
					<span class="text-nowrap">{server.password}</span>
				</span>
            </li>
        </ul>
    </div>
</div>
<div class="mws-panel grid_2 mws-collapsible">
    <div class="mws-panel-header">
        <span><i class="icon-legal"></i> Server Active Rules</span>
    </div>
    <div class="mws-panel-body no-padding">
        <ul class="mws-summary clearfix">
            <li>
                <span class="key"><i class="icon-balance"></i> Auto Balance</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_autobalanced}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-gun-forbidden"></i> Friendly Fire</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_friendlyfire}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-skull"></i> Team Kill Mode</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_tkmode}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-meter-fast"></i> Score Limit</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_scorelimit}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-alarm-check"></i> Time Limit</span>
                <span class="val">
					<span class="text-nowrap">{server.timelimit}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-target2"></i> Anti-Cheat</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_anticheat}</span>
				</span>
            </li>
        </ul>
    </div>
</div>
<div class="mws-panel grid_3 mws-collapsible">
    <div class="mws-panel-header">
        <span><i class="icon-cogs"></i> Game Settings</span>
    </div>
    <div class="mws-panel-body no-padding">
        <ul class="mws-summary clearfix">
            <li>
                <span class="key"><i class="icon-tags"></i> Ticket Ratio</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_ticketratio}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-sign-post"></i> Team Ratio</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_teamratio}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-flag"></i> Team 1</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_team1}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-flag2"></i> Team 2</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_team2}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-android"></i> Bots Enabled</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_bots}</span>
				</span>
            </li>
            <li>
                <span class="key"><i class="icon-chopper"></i> Vehicles</span>
                <span class="val">
					<span class="text-nowrap">{server.bf2_novehicles}</span>
				</span>
            </li>
        </ul>
    </div>
</div>
<div class="mws-panel grid_4 mws-collapsible clear">
    <div class="mws-panel-header">
        <span>
            <img style="margin: -3px 10px 0 5px" src="/ASP/frontend/images/armies/small/{team1flag}.png"/>
            {team1name}
        </span>
    </div>
    <div class="mws-panel-body no-padding">
        <table id="team2" class="mws-table">
            <thead>
            <tr>
                <th style="width: 7%">Rank</th>
                <th>Name</th>
                <th style="width: 10%">Score</th>
                <th style="width: 10%">Kills</th>
                <th style="width: 10%">Deaths</th>
                <th style="width: 10%">Ping</th>
            </tr>
            </thead>
            <tbody>
            {players1}
                <tr>
                    <td><img src="/ASP/frontend/images/ranks/rank_{rank}.gif"/></td>
                    <td>{gq_name}</td>
                    <td>{gq_score}</td>
                    <td>{gq_kills}</td>
                    <td>{gq_deaths}</td>
                    <td>{gq_ping}</td>
                </tr>
            {/players1}
            </tbody>
        </table>
    </div>
</div>
