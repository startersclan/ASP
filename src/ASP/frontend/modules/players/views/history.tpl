<div id="jui-global-message" class="alert" style="display: none;"></div>
<div class="mws-panel grid_8">
    <div class="mws-panel-header">
        <span><i class="icon-time"></i> Player Round History</span>
        <span id="playerId" style="display: none">{id}</span>
    </div>
    <div class="mws-panel-body no-padding">
        <table class="mws-datatable-fn mws-table">
            <thead>
                <tr>
                    <th style="width: 16%">Round Date</th>
                    <th style="width: 20%">Map Name</th>
                    <th style="width: 20%">Server</th>
                    <th style="width: 6%">Rank</th>
                    <th>Score</th>
                    <th>Kills</th>
                    <th>Deaths</th>
                    <th>Time</th>
                    <th>Team</th>
                    <th style="width: 5%">View</th>
                </tr>
            </thead>
        </table>
    </div>
</div>